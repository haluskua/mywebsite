{
  "name": "mywebsite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node-sass -w scss/ -o DIST/css/ --recursive",
    "prefix": "postcss DIST/css/main.css --use=autoprefixer --map=false --output=DIST/css/main.css",
    "minify": "cleancss --level=1 --source-map --source-map-inline-sources --output DIST/css/main.min.css DIST/main.css",
    "build": "npm run prefix && npm run minify",
    "deploy": "gh-pages -d DIST"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://haluskua.github.io/mywebsite",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^5.0.0",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3"
  }
}
