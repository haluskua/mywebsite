{
  "name": "mywebsite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "node-sass --output-style=expanded --source-map=true scss/main.scss DIST/css/main.css",
    "prefix": "postcss DIST/css/main.css --use=autoprefixer --map=false --output=DIST/css/main.css",
    "minify": "cleancss --level=1 --source-map --source-map-inline-sources --output DIST/css/main.min.css DIST/css/main.css",
    "dev": "npm run compile -- --watch",
    "build": "npm run compile && npm run prefix && npm run minify",
    "deploy": "gh-pages -d ../MYWEBSITE/DIST"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://haluskua.github.io/mywebsite",
  "dependencies": {
    "gh-pages": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3"
  }
}
